<header role="banner" data-theme="dim">
  <div class="container">
    <div class="site-head">
      <a href="/" id="logo">
        <%= site.metadata.title %>
      </a>

      <button 
        id="hamburger" 
        aria-label="Menu" 
        aria-expanded="false" 
        aria-controls="main-nav">
          Menu
      </button>

      <nav id="main-nav" class="main-nav">
          <ul role="list">
              <% site.data.nav.each do |item| %>
                  <li><%= link_to item.title, item.url %></li>
              <% end %>
          </ul>
      </nav>

    </div>
  </div>
</header>